#!/usr/bin/env bash

function error_handler() {
  echo "Exited with BAD EXIT CODE '${2}' in ${0} script at line: ${1}."
  exit "$2"
}
trap 'error_handler ${LINENO} $?' ERR
set -o errtrace -o errexit -o nounset -o pipefail

# HELP: kubernetes/k8s: basic kubernetes types that we care about

echo 'cm,cronjob,deploy,daemonset,hpa,pvc,ing,job,po,poddisruptionbudget,prometheusrule,role,rolebinding,rs,secret,serviceaccount,servicemonitors,statefulset,svc'
